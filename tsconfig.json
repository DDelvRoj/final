{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "ts-node": {
    "compilerOptions": {
      "allowJs": true,
      "target": "ESNext",
      "jsx": "react-native",
      "module": "CommonJS",
      "resolveJsonModule": true,
      "noEmit": true,
      "isolatedModules": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }
  },
  
  
}
